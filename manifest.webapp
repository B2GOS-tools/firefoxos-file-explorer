{
  "name": "SD Explorer",
  "version": "1.1",
  "description": "This app allows you to check your sd card!",
  "launch_path": "/index.html",
  "developer": {
    "name": "Ivan Lopez",
    "url": "https://github.com/greuze"
  },
  "permissions": {
    "device-storage:sdcard": {
      "description": "To read and write in the SD card file system",
      "access": "readwrite"
    }
  },
  "icons": {
    "60": "/icons/icon_60.png",
    "84": "/icons/icon_84.png",
    "128": "/icons/icon_128.png",
    "284": "/icons/icon_284.png"
  },
  "default_locale": "en",
  "type": "privileged"
}